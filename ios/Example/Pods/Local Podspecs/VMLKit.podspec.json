{
  "name": "VMLKit",
  "version": "0.0.1",
  "summary": "iOS implementation of https://visly.app",
  "homepage": "https://visly.app",
  "authors": {
    "Visly Inc.": "emil@visly.app"
  },
  "source": {
    "git": "https://github.com/vislyhq/vml-mobile.git",
    "tag": "0.0.1"
  },
  "license": {
    "type": "MIT"
  },
  "swift_version": "4.2",
  "platforms": {
    "ios": "10.0"
  },
  "source_files": "VMLKit/Classes/**/*",
  "dependencies": {
    "Kingfisher": [
      "~> 5.0.1"
    ]
  },
  "subspecs": [
    {
      "name": "VMLCore",
      "source_files": "Libraries/Headers/*.h",
      "vendored_libraries": "Libraries/libvml.a",
      "xcconfig": {
        "HEADER_SEARCH_PATHS": "${PODS_ROOT}/VMLCore/Libraries/Headers"
      },
      "preserve_paths": [
        "Libraries/libvml.a",
        "Libraries/Headers/libvml.h"
      ],
      "public_header_files": "Libraries/Headers/*.h",
      "requires_arc": false
    }
  ]
}
